{
  "name": "ati",
  "version": "0.1.0",
  "private": true,
  "homepage": "wordpress/wp-content/themes/ati-2018/frontpage",
  "scripts": {
    "start": "NODE_ENV=dev react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "lint": "./node_modules/eslint/bin/eslint.js ./src",
    "pre-build": "cp public/index.html public/_index.html && cp public/root-partial.html public/index.html",
    "post-build": "cp public/index.html public/root-partial.html && cp public/_index.html public/index.html && cp build/index.html build/index.php",
    "build": "npm run pre-build && NODE_ENV=dev react-scripts build && npm run post-build",
    "deploy": "tar cvzf /tmp/front.tgz build && scp /tmp/front.tgz pbh:/tmp"
  },
  "dependencies": {
    "faker": "^4.1.0",
    "modal-react-native-web": "^0.2.0",
    "nuka-carousel": "4.4.4",
    "react": "^16.8.5",
    "react-art": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-icons": "^3.4.0",
    "react-native-floating-action": "^1.14.2",
    "react-native-web": "^0.9.13",
    "react-scripts": "^2.1.8"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-jest": "23.6.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.91.0",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.5"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  }
}
